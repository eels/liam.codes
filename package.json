{
  "name": "liam.codes",
  "private": true,
  "description": "Source for my personal website, https://liam.codes",
  "author": "Liam Howell <liam@liam.codes>",
  "homepage": "https://liam.codes",
  "repository": "https://github.com/eels/liam.codes",
  "bugs": "https://github.com/eels/liam.codes/issues",
  "scripts": {
    "build": "next build && yarn build:sw",
    "build:sw": "rollup --config",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier './{src,test}/**/*.{ts,tsx}' --check",
    "format:fix": "yarn format --write",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:css": "stylelint './src/**/*.scss' --cache",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:js": "eslint './{src,test}/**/*.{ts,tsx}' --cache",
    "lint:js:fix": "yarn lint:js --fix",
    "prepare": "husky install",
    "test": "jest --coverage --passWithNoTests"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "chic-modules": "^1.0.7",
    "next": "^12.2.5",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "next-purge-css-modules": "^1.1.0",
    "prettier": "^2.7.1",
    "rollup": "^2.77.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.54.4",
    "stylelint": "^14.11.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.7.4"
  }
