{
  "name": "liam.codes",
  "private": true,
  "type": "module",
  "description": "Source for my personal website, https://liam.codes",
  "author": "Liam Howell <liam@liam.codes>",
  "homepage": "https://liam.codes",
  "repository": "https://github.com/eels/liam.codes",
  "bugs": "https://github.com/eels/liam.codes/issues",
  "scripts": {
    "build": "astro build",
    "clean": "rimraf \"dist\"",
    "dev": "astro dev",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./src/**/}*.{astro,cjs,js,json,ts}\" --check",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:js": "eslint \"./*.{cjs,js,ts}\" \"./src/**/*.{astro,js,json,ts}\" --cache",
    "lint": "yarn lint:js",
    "prepare": "husky install",
    "start": "astro preview"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "astro-icon": "next",
    "astro": "3.0.5",
    "astrojs-service-worker": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@prettier/multiline-arrays@npm:prettier-plugin-multiline-arrays": "1.1.3",
    "@types/node": "20.5.7",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "astro-compress": "2.0.14",
    "astro-critters": "2.0.11",
    "autoprefixer": "10.4.15",
    "eslint-config-semistandard": "17.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-astro": "0.29.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-n": "16.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-typescript-sort-keys": "3.0.0",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint": "8.48.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "postcss": "8.4.29",
    "prettier-plugin-astro": "0.8.0",
    "prettier": "2.8.7",
    "react": "18.2.0",
    "rimraf": "5.0.1",
    "svgo": "3.0.2",
    "tailwind-compose": "1.1.0",
    "tailwindcss": "3.3.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2",
    "vite-plugin-inject-preload": "1.3.3"
  }
}
