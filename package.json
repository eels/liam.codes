{
  "name": "liam.codes",
  "private": true,
  "type": "module",
  "description": "Source for my personal website, https://liam.codes",
  "author": "Liam Howell <liam@liam.codes>",
  "homepage": "https://liam.codes",
  "repository": "https://github.com/eels/liam.codes",
  "bugs": "https://github.com/eels/liam.codes/issues",
  "scripts": {
    "build": "astro build",
    "dev": "astro dev",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./src/**/}*.{astro,js,json,ts}\" --check",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:js": "eslint \"./*.{js,ts}\" \"./src/**/*.{astro,js,json,ts}\" --cache",
    "lint": "yarn lint:js",
    "prepare": "husky install",
    "start": "astro preview"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@astrojs/tailwind": "3.1.1",
    "astro": "2.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@prettier/multiline-arrays@npm:prettier-plugin-multiline-arrays": "1.1.3",
    "@types/node": "18.15.10",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "eslint-config-semistandard": "17.0.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-astro": "0.26.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unicorn": "45.0.2",
    "eslint": "8.36.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier-plugin-astro": "0.8.0",
    "prettier": "2.8.7",
    "tailwindcss": "3.3.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "5.0.2"
  }
}
