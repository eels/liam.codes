{
  "name": "liam.codes",
  "private": true,
  "description": "Source for my personal website, https://liam.codes",
  "author": "Liam Howell <liam@liam.codes>",
  "homepage": "https://liam.codes",
  "repository": "https://github.com/eels/liam.codes",
  "bugs": "https://github.com/eels/liam.codes/issues",
  "scripts": {
    "build": "next build && yarn build:sw",
    "build:sw": "rollup --config",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier './{src,test}/**/*.{ts,tsx}' --check",
    "format:fix": "yarn format --write",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:css": "stylelint './src/**/*.scss' --cache",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:js": "eslint './{src,test}/**/*.{ts,tsx}' --cache",
    "lint:js:fix": "yarn lint:js --fix",
    "prepare": "husky install",
    "test": "jest --coverage --passWithNoTests"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "chic-modules": "^1.0.7",
    "next": "^12.0.8",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "workbox-cacheable-response": "^6.4.2",
    "workbox-expiration": "^6.4.2",
    "workbox-routing": "^6.4.2",
    "workbox-strategies": "^6.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.1.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.9.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-testing-library": "^5.0.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "next-purge-css-modules": "^1.0.1",
    "prettier": "^2.5.1",
    "rollup": "^2.67.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.0",
    "stylelint": "^14.1.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.5.4"
  }
}
